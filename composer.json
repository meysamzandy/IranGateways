{
  "name": "meysam-znd/iran-gateways",
  "description": "Iranian laravel bank gateways",
  "license": "MIT",
  "keywords": [
    "iran",
    "gateways"
  ],
  "type": "library",
  "authors": [
    {
      "name": "meysam zandy",
      "email": "zandy.meysam@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0",
    "econea/nusoap": "^0.9.7"
  },

  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "MeysamZnd\\IranGateways\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MeysamZnd\\IranGateways\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MeysamZnd\\IranGateways\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
